    MAXSCALEDENOM 60000
    CLASS
        MAXSCALEDENOM 7000
        STYLE
            COLOR "<%= get_color("building","offset") %>"
            OFFSET 2 2
        END
        STYLE
            COLOR "<%= get_color("building") %>"
        END
        LABEL
        TYPE TRUETYPE FONT arial PARTIALS FALSE OUTLINEWIDTH 1
        MAXSCALEDENOM 7000
            SIZE 7
            COLOR "<%= get_color("building_label") %>"
            OUTLINECOLOR "<%= get_color("building_label", "outline") %>"
            PRIORITY 1
            WRAP ' '
            #MAXLENGTH 6
            ALIGN CENTER
        END
    END
    CLASS
        MAXSCALEDENOM 11000
        STYLE
            COLOR "<%= get_color("building","offset") %>"
            OFFSET 1 1
        END
        STYLE
            COLOR "<%= get_color("building") %>"
        END
    END
    CLASS
        STYLE
            COLOR "<%= get_color("building","offset") %>"
            # OUTLINECOLOR "<%= get_color("building","offset") %>"
            # OUTLINEWIDTH 0.5
        END
    END
