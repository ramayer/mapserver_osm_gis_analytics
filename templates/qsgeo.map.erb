    CLASS
        STYLE
            COLOR [color]
            OPACITY 20
        END
    END
END

LAYER
    STATUS ON GROUP "default"
    PROJECTION "init=epsg:3857" END
    <%= default_connection() %>
    TYPE POLYGON
    NAME "polys_outlines"
    DATA "geom from (select id,geom,featurename,color from layers_from_shapefiles) as foo using unique id using srid=900913"
